<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="820.0" prefWidth="1089.0">
      <children>
         <VBox alignment="CENTER" layoutX="322.0" layoutY="220.0" prefHeight="358.0" prefWidth="446.0" spacing="20.0">
            <children>
               <Label fx:id="welcomeLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="94.0" prefWidth="426.0" text="welcome to your bullet journal" textAlignment="CENTER">
                  <font>
                     <Font name="Avenir Book" size="24.0" />
                  </font>
               </Label>
               <Button fx:id="createNewFile" mnemonicParsing="false" prefHeight="35.0" prefWidth="150.0" style="-fx-background-color: DBDFAA;" text="create new file">
                  <font>
                     <Font name="Avenir Book" size="13.0" />
                  </font>
               </Button>
               <Button fx:id="openExistingFile" mnemonicParsing="false" prefHeight="36.0" prefWidth="150.0" style="-fx-background-color: DBDFAA;" text="open existing file">
                  <font>
                     <Font name="Avenir Book" size="13.0" />
                  </font>
               </Button>
               <Button fx:id="openFileAsTemplate" mnemonicParsing="false" prefHeight="36.0" prefWidth="150.0" style="-fx-background-color: DBDFAA;" text="open file as template">
                  <font>
                     <Font name="Avenir Book" size="13.0" />
                  </font>
               </Button>
            </children>
            <padding>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
         </VBox>
      </children></AnchorPane>
</Scene>
